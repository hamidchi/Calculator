<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <solid android:color="@color/colorClearButtonBg"/>
            <stroke android:color="@color/colorButtonStroke" android:width="@dimen/narrowButtonStroke"></stroke>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="@color/colorClearButtonBgPressed"/>
            <stroke android:color="@color/colorButtonStroke" android:width="@dimen/narrowButtonStroke"></stroke>
        </shape>
    </item>
</selector>